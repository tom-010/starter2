{
  "name": "starter2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "rm dev.log ; FORCE_COLOR=1 concurrently --kill-others -n \"VITE,WORKER,PY\" -c \"bgBlue.bold,bgMagenta.bold,bgYellow.bold\" \"npm run dev:vite\" \"npm run dev:worker\" \"npm run dev:py\" 2>&1 | tee dev.log",
    "dev:vite": "react-router dev",
    "dev:worker": "npx tsx scripts/worker.ts",
    "dev:py": "cd py && uv run python -m uvicorn main:app --reload --host 0.0.0.0 --port 8001",
    "build": "react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "db:generate": "prisma generate",
    "db:migrate": "prisma db push",
    "db:studio": "prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "npx tsx prisma/seed.ts && playwright test",
    "test:e2e:ui": "npx tsx prisma/seed.ts && playwright test --ui"
  },
  "dependencies": {
    "@conform-to/react": "^1.15.0",
    "@conform-to/zod": "^1.15.0",
    "@hey-api/client-fetch": "^0.10.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-router/node": "^7.10.1",
    "@react-router/serve": "^7.10.1",
    "better-auth": "^1.4.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "graphile-worker": "^0.16.6",
    "isbot": "^5.1.31",
    "lucide-react": "^0.561.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.10.1",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.71.0",
    "@playwright/test": "^1.57.0",
    "@react-router/dev": "^7.10.1",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^22",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "jsdom": "^27.3.0",
    "prisma": "^7.2.0",
    "shadcn": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.16"
  }
}
